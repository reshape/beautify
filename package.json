{
  "name": "reshape-beautify",
  "description": "a reshape plugin that pretty-prints your html",
  "version": "0.1.2",
  "author": "reshape",
  "ava": {
    "verbose": "true"
  },
  "bugs": "https://github.com/reshape/beautify/issues",
  "devDependencies": {
    "ava": "^1.2.0",
    "coveralls": "^2.11.12",
    "nyc": "^10.0.0",
    "reshape": "^0.5.0",
    "snazzy": "^6.0.0",
    "standard": "^10.0.0"
  },
  "engines": {
    "node": ">= 6"
  },
  "homepage": "https://github.com/reshape/beautify",
  "keywords": [
    "beautify",
    "indent",
    "reshape-plugin"
  ],
  "license": "MIT",
  "main": "lib",
  "repository": "reshape/beautify",
  "scripts": {
    "coverage": "nyc ava && nyc report --reporter=html && open ./coverage/index.html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard | snazzy",
    "test": "nyc ava"
  }
}
